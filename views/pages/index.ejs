<!DOCTYPE html>
<html lang="en">
<%- include('../partials/head')%>

    <body>
        <%- include('../partials/header')%>
            <main>
                <section class="container">
                    
                   <% registro.forEach( (registro) => { %>
        
                    <div class="wrapper">
                        <div class="card">
                            <img src="<%= registro.imagesCar%>" alt="Veiculo">
                        </div>
                        <div class="card">
                            <h2><%= registro.id%></h2>
                        </div>
                        <div class="card">
                            <h2><%= registro.descriptionCar%></h2>
                            <p><%= registro.yearCar%></p>
                        </div>
                        <div class="card">

                            <h2><%= registro.locationCar%></h2>
                        </div>
                        <div class="card">

                            <h2><%= registro.MileageCar%></h2>
                        </div>
                        <div class="card">

                            <h2><%= registro.ConductorCar%></h2>
                        </div>
                        <div class="card">

                            <a href="http://www.google.com"><button>
                                    Consulta</button> </a>

                        </div>

                    </div>
                <% }) %> 
                </section>

            </main>
            <%- include('../partials/footer')%>

    </body>

</html>