<!DOCTYPE html>
<html lang="en">
<%- include('../partials/head')%>

    <body>
        <%- include('../partials/header')%>
            <main>
                <section class="container">

                    <% registro.forEach( (registro)=> { %>

                        <div class="wrapper">
                            <div class="card">
                                <img src="<%= registro.imagesCar%>" alt="Veiculo">
                            </div>
                            <div class="card">
                                <h2>
                                    <%= registro.id%>
                                </h2>
                            </div>
                            <div class="card">
                                <h2>
                                    <%= registro.descriptionCar%>
                                        <p>
                                            <%= registro.yearCar%>
                                        </p>
                                </h2>


                            </div>
                            <div class="card">

                                <h2>
                                    <%=registro.locationCar%>
                                </h2>
                            </div>
                            <div class="card">

                                <h2>
                                    <%=registro.mileageCar%>
                                </h2>
                            </div>
                            <div class="card">

                                <h2>
                                    <%= registro.conductorCar%>
                                </h2>
                            </div>
                            <div class="card">

                                <a href="/car<%= registro.id%>"><button>
                                        <h2> Consulta</h2>
                                    </button> </a>

                            </div>

                        </div>
                        <% }) %>
                </section>

            </main>
            <%- include('../partials/footer')%>

    </body>

</html>